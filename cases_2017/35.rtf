\pard\plain \ltrpar\s20\qj \fi-454\li454\ri0\sa240\sl-280\slmult0\widctlpar\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\wrapdefault\aspalpha\aspnum\faauto\adjustright\rin0\lin454\itap0\pararsid7805801 \rtlch\fcs1 
