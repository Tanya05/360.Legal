\par {\pntext\pard\plain\ltrpar \s27 \rtlch\fcs1 \af0\afs20 \ltrch\fcs0 \fs20\lang3081\langfe1033\langfenp1033\insrsid8221512\charrsid10757525 \hich\af0\dbch\af0\loch\f0 64\tab}}\pard \ltrpar\s27\qj \fi-720\li0\ri0\sa260\sl-280\slmult0
